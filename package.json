{
  "name": "pos-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "camelcase-keys-deep": "^0.1.0",
    "classnames": "^2.2.5",
    "cross-env": "^5.1.5",
    "d3": "5.0.0",
    "d3-selection-multi": "^1.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "flow-bin": "^0.72.0",
    "formik": "^0.11.11",
    "husky": "^0.14.3",
    "is-equal": "^1.5.5",
    "jest-enzyme": "^6.0.0",
    "jquery": "^3.3.1",
    "lint-staged": "^7.1.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "moment-timezone": "^0.5.17",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "numeral": "^2.0.6",
    "papaparse": "^4.4.0",
    "prettier": "^1.12.1",
    "prop-types": "^15.6.1",
    "qs": "^6.5.2",
    "query-string": "5",
    "react": "^16.3.1",
    "react-addons-update": "^15.6.2",
    "react-bootstrap-typeahead": "^3.1.4",
    "react-day-picker": "^7.1.9",
    "react-dom": "^16.3.1",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "react-redux-i18n": "^1.9.2",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "react-table": "^6.8.6",
    "react-test-renderer": "^16.3.2",
    "recompose": "^0.27.1",
    "redux": "^3.7.2",
    "redux-actions": "^2.3.0",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "redux-thunk-fsa": "^4.0.1",
    "reselect": "^3.0.1",
    "source-map-explorer": "^1.5.0",
    "tiny-emitter": "^2.0.2",
    "treo": "^0.5.1",
    "vend-number": "^3.0.1",
    "yup": "^0.24.1",
    "zxcvbn": "^4.4.2"
  },
  "scripts": {
    "------- Locales scripts:": "",
    "locales:compile": "cd locales && npm run compile-locales",
    "locales:move": "copyfiles -f locales/dist/*.json public/locales",
    "------- Test scripts:": "",
    "test": "react-scripts test --env=jsdom",
    "------- Build scripts:": "",
    "postbuild:css": "npm rebuild node-sass",
    "delete:css": "del-cli ./src/**/*.css",
    "build:rombostrap-css": "node-sass-chokidar src/rombostrap/styles/index.scss -o src/rombostrap",
    "build:component-css": "node-sass-chokidar src/app -o src/app",
    "build:css": "run-s build:rombostrap-css build:component-css",
    "watch:css": "node-sass-chokidar --include-path src --include-path ./node_modules/ src -o src/ --watch --recursive",
    "start:css": "run-s delete:css build:css",
    "start:js": "react-scripts start",
    "start": "cross-env REACT_APP_ENV=development run-s copy:env locales:compile locales:move postbuild:css start:css start:js",
    "clean": "shx rm -rf build",
    "copy:env": "cross-env shx cp ./config/${REACT_APP_ENV}.env ./.env",
    "build": "react-scripts build",
    "build:staging": "cross-env REACT_APP_ENV=staging run-s clean copy:env locales:compile locales:move build",
    "build:production": "cross-env REACT_APP_ENV=production run-s clean copy:env locales:compile locales:move build",
    "eject": "react-scripts eject",
    "------- Deploy scripts:": "",
    "deploy:staging:s3": "aws s3 sync build/ s3://rombopos",
    "deploy:production:s3": "aws s3 sync build/ s3://romboprodpos",
    "invalidate:staging": "aws cloudfront create-invalidation --distribution-id E103VBB4Y8HJ3K --paths /*",
    "invalidate:production": "aws cloudfront create-invalidation --distribution-id E103VBB4Y8HJ3K --paths /*",
    "deploy:staging": "run-s build:staging deploy:staging:s3 invalidate:staging",
    "deploy:production": "run-s build:production deploy:production:s3 invalidate:production",
    "------- Other scripts:": "",
    "flow": "flow",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.5",
    "@storybook/addon-links": "^3.4.5",
    "@storybook/addons": "^3.4.5",
    "@storybook/react": "^3.4.5",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "del-cli": "^1.1.0",
    "glob": "^7.1.2",
    "properties-parser": "^0.3.1",
    "shx": "^0.2.2",
    "yargs": "^11.0.0"
  }
}
